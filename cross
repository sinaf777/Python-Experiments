def sq1():
    n = 4
    for i in range (n):
        for j in range (n):
            print(' ', end = ' ')
        for j in range (n):
            print('*', end = ' ')
        print()
def rec():
    n = 4
    m = 12
    for i in range(n):
        for j in range(m):
            print('*', end = ' ')
        print()
sq1()
rec()
sq1()
sq1()
